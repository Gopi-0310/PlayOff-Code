{
  "name": "playoffs",
  "version": "1.0.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm install && npm run generateIconsAndSplash && ng build && npx cap sync && npm run apk",
    "release": "npm version patch && npm run build",
    "generateIconsAndSplash": "cordova-res android --skip-config --copy && cordova-res ios --skip-config --copy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "apk": "cd android && ./gradlew assembleDebug && pwd && cp ./app/build/outputs/apk/debug/app-debug.apk ../apk/Playoffs_v${npm_package_version}.apk"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular-material-extensions/google-maps-autocomplete": "^8.1.2",
    "@angular/animations": "^14.2.5",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.0.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.1.2",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@capacitor-community/admob": "^4.0.0",
    "@capacitor-community/contacts": "^1.1.3",
    "@capacitor/android": "^4.0.1",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.0.1",
    "@capacitor/ios": "^4.0.1",
    "@capacitor/network": "^4.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@rxweb/reactive-form-validators": "^13.0.0",
    "browser-module": "^0.0.1",
    "deps": "^1.0.0",
    "mat-timepicker": "^5.1.7",
    "ngx-material-timepicker": "^5.5.3",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~14.1.2",
    "@angular/compiler": "^14.0.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.0.0",
    "@angular/localize": "^14.1.2",
    "@capacitor/cli": "latest",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~4.0.0",
    "cordova-res": "^0.15.4",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
